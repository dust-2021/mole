{
  "name": "mole",
  "version": "1.0.0",
  "main": "dist/main/electron-main.js",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on http://localhost:3000 && tsc && electron .\"",
    "build": "vite build && tsc && electron-builder"
  },
  "build": {
    "appId": "com.x.mole",
    "productName": "mole",
    "directories": {
      "output": "locals/pack/${version}"
    },
    "files": [
      "dist/main/**/*",
      "dist/renderer/**/*",
      "dist/shared/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "asar": true,
    "asarUnpack": [
      "**/*.node"
    ],
    "extraResources": [
      {
        "from": "lib/src/wireguard.dll",
        "to": "wireguard.dll"
      },
      {
        "from": "lib/src/wireguard_handle.dll",
        "to": "wireguard_handle.dll"
      },
      {
        "from": "lib/src/WinDivert.dll",
        "to": "WinDivert.dll"
      },
      {
        "from": "lib/src/WinDivert64.sys",
        "to": "WinDivert64.sys"
      }
    ],
    "win": {
      "icon": "src/assets/mole.ico"
    }
  },
  "keywords": [],
  "author": "li",
  "license": "ISC",
  "description": "mole for NAT",
  "devDependencies": {
    "@types/base64-url": "^2.2.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/electron": "^1.4.38",
    "@types/node": "^22.13.4",
    "@types/ws": "^8.18.1",
    "electron": "^34.2.0",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.27.2",
    "fast-glob": "^3.3.3",
    "typescript": "^5.7.3",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-vue-components": "^28.4.1",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@noble/curves": "^2.0.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.9",
    "buffer": "^6.0.3",
    "element-plus": "^2.9.7",
    "getmac": "^6.6.0",
    "iconv-lite": "^0.7.2",
    "koffi": "^2.14.0",
    "pinia": "^3.0.2",
    "tweetnacl": "^1.0.3",
    "uuid": "^11.1.0",
    "vite": "^6.1.1",
    "vue-router": "^4.5.0",
    "wait-on": "^8.0.2",
    "winston": "^3.17.0",
    "ws": "^8.18.2"
  }
}
