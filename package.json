{
  "name": "mole",
  "version": "1.0.0",
  "main": "dist/main/electron-main.js",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on http://localhost:3000 && tsc && electron .\"",
    "build": "vite build && tsc && electron-builder"
  },
  "build": {
    "extraFiles": [
      {
        "from": "lib/wireguard/amd64/${file}",
        "to": "resources/wireguard/${file}",
        "filter": [
          "wireguard.dll"
        ]
      }
    ]
  },
  "keywords": [],
  "author": "li",
  "license": "ISC",
  "description": "mole for NAT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/electron": "^1.4.38",
    "@types/node": "^22.13.4",
    "@types/ws": "^8.18.1",
    "electron": "^34.2.0",
    "electron-builder": "^26.0.12",
    "fast-glob": "^3.3.3",
    "typescript": "^5.7.3",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-vue-components": "^28.4.1",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.9",
    "buffer": "^6.0.3",
    "element-plus": "^2.9.7",
    "getmac": "^6.6.0",
    "koffi": "^2.14.0",
    "pinia": "^3.0.2",
    "uuid": "^11.1.0",
    "vite": "^6.1.1",
    "vue-router": "^4.5.0",
    "wait-on": "^8.0.2",
    "winston": "^3.17.0",
    "ws": "^8.18.2"
  }
}
